{
   "author": "Jake Eaton",
   "background": {
      "persistant": false,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "content.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "Center images similar to Firefox",
   "manifest_version": 2,
   "name": "Image Center",
   "permissions": [ "<all_urls>", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.6"
}
